public class Biome {
    protected String biome = "Default biome";

    public Biome(String biome) {
        if (biome != null) { this.biome = biome;}
    }

    public String getBiome() {
        return this.biome;
    }

    public String toString() {
        return "This biome is a" + this.biome;
    }

}